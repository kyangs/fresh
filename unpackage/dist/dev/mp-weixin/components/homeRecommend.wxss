
.homeRecommend{
	width: 100%;
	height: auto;
	background: #FFFFFF;
	border-radius: 15rpx;
}
.homeRecommend-name{
	width: 100%;
	height: 85rpx;
	padding-left: 32rpx;
	box-sizing: border-box;
	line-height: 85rpx;
	font-weight: bold;
	color: #1B1B1B;
	font-size: 30rpx;
}
.homeRecommend-box{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	        justify-content: flex-start;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.homeRecommend-box-list{
	width: 25%;
	height: auto;
	margin-bottom: 65rpx;
}
.homeRecommend-box-list-icon{
	width: 100%;
	height: 86rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.homeRecommend-box-list-icon-image{
	width: 100%;
	height: 86rpx;
	/* display: flex;
	justify-content: center; */
	/* align-items: center; */
}
.homeRecommend-box-list-icon-image image{
	width: 45rpx;
	height: 45rpx;
}
.homeRecommend-box-list-icon text{
	font-size: 46rpx;
	text-align: center;
}
.icon-one{
	color: #FC6226;
}
.icon-two{
	color: #22CD29;
}
.icon-three{
	color: #3D9FFC;
}
.homeRecommend-box-list-name{
	font-size: 22rpx;
	color: #1E1F23;
	text-align: center;
}

