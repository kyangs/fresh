
.mainNavBox{
	width: 100%;
	height: auto;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	        justify-content: flex-start;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.mainNavBox-list{
	width: 140rpx;
	height: 168rpx;
	padding-top: 10rpx;
	box-sizing: border-box;
}
.mainNavBox-list-img{
	width: 100%;
	height: 108rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.mainNavBox-list-img image{
	width: 108rpx;
	height: 108rpx;
}
.mainNavBox-list-text{
	width: 100%;
	height: 50rpx;
	padding-top: 12rpx;
	box-sizing: border-box;
	font-size: 22rpx;
	text-align: center;
	color: #030302;
	font-weight: 700;
}

